\r\n        @extend%part;\r\n        border: 10px solid white;\r\n        border-radius: 20px;\r\n        display: flex;\r\n\r\n\r\n        \r\n        &__aiHand{    \r\n            background-image: url('../graphic/aiRock.png');\r\n            background-size:50%;\r\n            background-position-x: center;\r\n            background-position-y: bottom;\r\n            background-repeat: no-repeat;\r\n            width: 50%;\r\n            animation: backgroundAnimationScale 0.2s infinite alternate;\r\n        }\r\n        &__userHand{\r\n            background-image: url('../graphic/userRock.png');\r\n            background-size:50%;\r\n            background-position-x: center;\r\n            background-position-y: bottom;\r\n            background-repeat: no-repeat;\r\n            width: 50%;\r\n        }\r\n    }\r\n    &__right{\r\n        @extend%part;\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: flex-start;\r\n        text-align: center;\r\n        color: white;\r\n        \r\n        &__weapons{ \r\n            min-height: 30vh;\r\n            border: 10px solid rgba(255, 255, 255, 0);\r\n            border-radius: 20px;\r\n            display: flex;\r\n            justify-content: space-evenly;\r\n            align-items: center;\r\n\r\n            \r\n            &__item{\r\n                padding: 20px;\r\n                border-radius: 20px;\r\n                width: 20%;\r\n                cursor: pointer;\r\n                background-color: rgba(128, 128, 128, 0.349);\r\n                border: 5px solid rgb(212, 210, 210) ;\r\n                transition: all .1s ease-in-out;\r\n\r\n                & img{\r\n                    max-height: 10vh;\r\n\r\n                }\r\n                &:hover{\r\n                    transform: scale(1.07);\r\n                    background-color: rgba(100, 223, 52, 0.733);\r\n                    border-color: #fff;\r\n                }\r\n            }\r\n        }\r\n        &__startbtn{\r\n            width: 15vw;\r\n            padding: 10px 0 ;\r\n            background-color: transparent;\r\n            border-style: solid;\r\n            border-radius: 30px;\r\n            border-color: #fff;\r\n            font-size: 30px;\r\n            border-width: 5px;\r\n            align-self: center;\r\n            transition: all .1s ease-in-out;\r\n            color: #fff;\r\n            cursor: pointer;\r\n\r\n            &:hover{\r\n                transform: scale(1.07);\r\n                background-color: rgba(100, 223, 52, 0.733);\r\n                border-color: #fff;\r\n            }\r\n        }\r\n    }\r\n\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"15ad5b1d685919fb586880b5011f0f65","cacheData":{"env":{}}}